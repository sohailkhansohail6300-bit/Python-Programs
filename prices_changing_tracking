prices=[]


while True:
    a=input('Enter the prices or (q) to quit : ')
    if a=='q':
        break
    else:
        prices.append(float(a))
    

    for i in range(1,len(prices)):
        change=prices[i]-prices[i-1]
        if change>0:
            print('prices is incearsed')
        elif change<0:
            print('prices is decrease')
        else:
            print('there is no change')


